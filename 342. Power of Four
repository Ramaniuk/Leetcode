class Solution {
    public boolean isPowerOfFour(int num) {
        
        if (num <=0)
            return false;   
        
        double x = Math.log(num)/Math.log(4);        
        int y = (int) x;
        double a = Math.pow(4, y);           
        int z = (int) a;        
        return num == z;     
        
        
    }
}
