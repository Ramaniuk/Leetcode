class Solution {
    public int fib(int N) {
        int a = 0;
        int b = 1;
        
        int fib = 0;
        if (N == 0 || N == 1) return N;
        
        for (int i = 1; i < N; i++) {
            fib = a + b;
 
            a = b;
 
            b = fib;
            
                                    }
        return fib;

    }
}
